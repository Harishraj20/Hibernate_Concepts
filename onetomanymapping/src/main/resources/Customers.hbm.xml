<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hbm:hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hbm:hibernate-mapping xmlns:hbm="http://www.hibernate.org/xsd/hibernate-mapping"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                       xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping
                                           http://www.hibernate.org/xsd/hibernate-mapping-3.0.xsd">



   <class name="com.practices.Customers" table="Customers_db">
       <id name="account_Number" column="account_Number">
           <generator class="identity"/>
       </id>
       <property name="customerName" column="customer_Name"/>
       <property name="balance" column="balance"/>
       <many-to-one name="bank" class="com.practices.Banks" column="IFSC_code" not-null="true"/>
   </class>



</hbm:hibernate-mapping>
